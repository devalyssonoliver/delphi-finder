unit SearchMore;

interface

uses
  System.SysUtils, System.Classes, Vcl.Controls, Vcl.StdCtrls, Vcl.Buttons;

type
  TSearchMore = class(TBitBtn)
  private
    { Private declarations }
  protected
    { Protected declarations }
  public
    { Public declarations }
    constructor Create(AOwner: TComponent); override;
    destructor  Destroy; override;
  published
    { Published declarations }
  end;

procedure Register;

implementation

procedure Register;
begin
  RegisterComponents('CursoComponenteDelphi', [TSearchMore]);
end;

{ TSearchMore }

constructor TSearchMore.Create(AOwner: TComponent);
begin
  inherited Create(AOwner);
  Caption := '...';
  Width   := 25;
  Height  := 25;


end;

destructor TSearchMore.Destroy;
begin

  inherited Destroy;
end;

end.
